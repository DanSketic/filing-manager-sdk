{
	"name": "@gveditor/sdk",
	"version": "0.2.4",
	"description": "Graviton SDK to develop plugins.",
	"main": "index.js",
	"bin": {
		"gvsdk": "src/cli.js"
	},
	"scripts": {
		"test:release:plugin": "node src/cli.js --target plugin --entry ./test/plugin/package.json --mode release",
		"test:watch:plugin": "node src/cli.js --target plugin --entry ./test/plugin/package.json",
		"test:release:iconpack": "node src/cli.js --target iconpack --entry ./test/iconpack/package.json --mode release",
		"test:release:theme": "node src/cli.js --target theme --entry ./test/theme/package.json --mode release"
	},
	"author": "Marc Esp√≠n Sanz <mespinsanz@gmail.com>",
	"license": "MIT",
	"repository": "https://github.com/Graviton-Code-Editor/graviton-sdk",
	"bugs": "https://github.com/Graviton-Code-Editor/graviton-sdk/issues",
	"dependencies": {
		"commander": "^5.0.0",
		"elliptic": "^6.5.3",
		"fs-extra": "^9.0.0",
		"lodash": "^4.17.19",
		"ncp": "^2.0.0",
		"parcel-bundler": "^1.12.4",
		"zip-a-folder": "0.0.12"
	}
}
