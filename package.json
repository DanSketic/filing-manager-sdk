{
	"name": "@gveditor/sdk",
	"version": "0.3.4",
	"description": "Graviton SDK to develop plugins.",
	"main": "bin/index.js",
	"bin": {
		"gvsdk": "src/cli.js"
	},
	"exports": {
		".": "./bin/index.js",
		"./types": "./types/index.ts"
	},
	"scripts": {
		"prepare": "npm run build",
		"build": "tsc  --project . ",
		"build:types": "tsc types/index",
		"test:release:plugin": "node src/cli.js --target plugin --project ./test/plugin --mode release",
		"test:watch:plugin": "node src/cli.js --target plugin --project ./test/plugin",
		"test:release:iconpack": "node src/cli.js --target iconpack --project ./test/iconpack --mode release",
		"test:release:theme": "node src/cli.js --target theme --project ./test/theme --mode release"
	},
	"author": "Marc Esp√≠n Sanz <mespinsanz@gmail.com>",
	"license": "MIT",
	"repository": "https://github.com/Graviton-Code-Editor/graviton-sdk",
	"bugs": "https://github.com/Graviton-Code-Editor/graviton-sdk/issues",
	"dependencies": {
		"commander": "^5.0.0",
		"elliptic": "^6.5.3",
		"fs-extra": "^9.0.0",
		"lodash": "^4.17.19",
		"ncp": "^2.0.0",
		"ts-loader": "^8.0.3",
		"typescript": "^4.0.2",
		"webpack": "^4.44.1",
		"zip-a-folder": "0.0.12"
	},
	"devDependencies": {
		"@types/node": "^14.6.2",
		"webpack-cli": "^3.3.12"
	}
}
